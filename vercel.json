{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "api/server.js": {
      "maxDuration": 30 
    },
    "/*.js": {
      "maxDuration": 15 
    }
  }
}
